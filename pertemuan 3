        "#konversi waktu, total detik ke jam:menit:detik\n",
        "\n",
        "#variabel\n",
        "total_detik = int(input(\"masukkan total detik     = \"))\n",
        "\n",
        "#konversi\n",
        "jam         = total_detik // 3600\n",
        "sisa_detik  = total_detik % 3600\n",
        "menit       = sisa_detik // 6\n",
        "detik       = sisa_detik % 60\n",
        "\n",
        "#output\n",
        "print(f\"hasil konversi           = {jam} jam,{menit} menit, {detik} detik\")\n"
      ],
