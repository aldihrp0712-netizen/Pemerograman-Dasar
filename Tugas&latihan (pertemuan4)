        "print (\"\\n===SELAMAT DATANG DI KOTOR (KOPI TOGAR) ===\")\n",
        "print (\"Menu Minuman & Makanan :  \")\n",
        "print (\"1. Kopi kenangan Aldi   - Rp 21.000\")\n",
        "print (\"2. Kopi Black Aren        - Rp 19.000\")\n",
        "print (\"3. Roti Cokelat Hot       - Rp 12.000\")\n",
        "print (\"======================================\")\n",
        "\n",
        "pilihan = int(input(\"Masukkan Pilihan Anda    : \"))\n",
        "\n",
        "if pilihan == 1 :\n",
        "  nama_pesanan = \"Kopi KOTOR\"\n",
        "  harga        = 21.000\n",
        "elif pilihan == 2 :\n",
        "  nama_pesanan = \"Kopi Black Aren\"\n",
        "  harga        = 19.000\n",
        "elif pilihan == 3 :\n",
        "  nama_pesanan = \"Roti Cokelat Hot\"\n",
        "  harga        = 12.000\n",
        "\n",
        "else:\n",
        "  print(\"Pilihan tidak tersedia\")\n",
        "  harga        = 0\n",
        "  nama_pesanan = \"Tidak Ada Pesanan\"\n",
        "\n",
        "if harga > 0 :\n",
        "  print(f\"Pesanan Anda Adalah {nama_pesanan} dengan harga {harga} \")\n",
        "\n",
        "  print(\"\\nKategori Diskon\")\n",
        "  print(\"1. Diskon 70%\")\n",
        "  print(\"2. Diskon 50%\")\n",
        "  print(\"3. Diskon 30%\")\n",
        "  pilih_diskon = int(input(\"Masukkan Pilihan Diskon (1-3) : \"))\n",
        "\n",
        "  if  pilih_diskon == 1 :\n",
        "    diskon = 0.7\n",
        "  elif pilih_diskon == 2 :\n",
        "    diskon = 0.5\n",
        "  elif pilih_diskon == 3 :\n",
        "    diskon = 0.3\n",
        "  else:\n",
        "    diskon = 0\n",
        "    print(\"Tidak ada diskon yang dipilih.\")\n",
        "\n",
        "  total_diskon = harga* diskon\n",
        "  total_bayar  = harga - total_diskon\n",
        "\n",
        "  print(f\"Harga Sebelum diskon        : Rp {harga}\")\n",
        "  print(f\"Total diskon yang diperoleh : Rp {total_diskon}\")\n",
        "  print(f\"Total yang harus dibayar    : Rp {total_bayar}\")\n",
        "\n",
        "  print (\"\\n===....TERIMA KASIH SUDAH BELANJA DI KOTOR (KOPI TOGAR) .....===\")\n",
        "  print (\"\\n=================================================================\")"
      ],
