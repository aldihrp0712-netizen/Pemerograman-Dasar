        "def hitung_rata_rata (data_nilai) :\n",
        "  \"\"\"Menghitung rata-rata nilai dengan validasi rentang nilai (0-100).\"\"\"\n",
        "  total = 0\n",
        "  jumlah_valid = 0\n",
        "\n",
        "  for nilai in data_nilai :\n",
        "    if 0 <= nilai <= 100:\n",
        "      total += nilai\n",
        "      jumlah_valid += 1\n",
        "\n",
        "    else :\n",
        "      print(f\"Nilai tidak valid: {nilai} (diabaikan)\")\n",
        "\n",
        "  if jumlah_valid > 0:\n",
        "      return total / jumlah_valid\n",
        "\n",
        "  else :\n",
        "      return 0\n",
        "nilai_praktikan = [85,120,92,-10,70]\n",
        "print(f\"NIlai rata-rata kelas (valid): {hitung_rata_rata(nilai_praktikan)}\")"
      ]
    },
