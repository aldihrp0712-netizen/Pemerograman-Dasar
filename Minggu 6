        "def hitung_volume_balok(panjang, lebar, tinggi) :\n",
        "  \"\"\"Mengitung volume balok dari panjang, lebar, dan tinggi.\"\"\"\n",
        "  return panjang * lebar * tinggi\n",
        "\n",
        "  p = 4\n",
        "  l = 3\n",
        "  t = 2\n",
        "\n",
        "  print (f\"volume balok dengan ukuran {p} x {1} x {t} adalah: {hitung_volume_balok(p,l,t)}\")\n"
      ]
